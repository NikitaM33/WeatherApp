{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "prefer-const": "error",
        "no-console": "warn",
        "semi": "warn",
        "quotes": ["warn", "single"],
        "jsx-quotes": ["warn", "prefer-double"],
        "indent": ["warn", 2],
        "comma-dangle": ["error", "always-multiline"]
    }
}